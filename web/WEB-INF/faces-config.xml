<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>allParts</from-outcome>
            <to-view-id>/allParts.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>newProject</from-outcome>
            <to-view-id>/newProject.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>projects</from-outcome>
            <to-view-id>/projects.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>signUp</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.testToolsDB</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.login</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>allParts</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.allParts</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>part</managed-bean-name>
        <managed-bean-class>beanPackage.part</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>editParts</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.editParts</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/allParts.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editParts.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editParts.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/allParts.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cart</from-outcome>
            <to-view-id>/updateCart.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>projects</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.projects</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>project</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.project</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>signup</from-outcome>
            <to-view-id>/signUp.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>idem</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/signUp.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/projects.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/projectManager.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>newProject</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.newProject</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/newProject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/projects.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>projectManager</managed-bean-name>
        <managed-bean-class>ManagedbeanPackage.projectManager</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/updateCart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/editParts.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
